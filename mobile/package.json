{
  "name": "ghana-school-bell-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:eas": "eas build -p android --profile production",
    "build:ios:eas": "eas build -p ios --profile production",
    "build:android:local": "expo prebuild --platform android && cd android && ..\\gradlew.bat clean assembleRelease",
    "build:android:debug:local": "expo prebuild --platform android && cd android && ..\\gradlew.bat clean assembleDebug",
    "clean:android": "cd android && ..\\gradlew.bat clean"
  },
  "dependencies": {
    "expo": "~48.0.21",
    "expo-av": "^13.0.0",
    "expo-notifications": "^0.18.0",
    "expo-task-manager": "^10.0.0",
    "expo-file-system": "^15.3.0",
    "expo-sharing": "^11.1.0",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "^6.7.3",
    "react-native-get-random-values": "^1.8.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "uuid": "^9.0.0",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.71.0",
    "@types/uuid": "^9.0.1",
    "@babel/core": "^7.20.0"
  }
}
