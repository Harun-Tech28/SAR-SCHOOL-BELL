{
  "engines": {
    "webspeech": {
      "enabled": true,
      "priority": 1,
      "name": "Web Speech API",
      "description": "System voices via Web Speech API",
      "requiresBundle": false
    },
    "piper": {
      "enabled": false,
      "priority": 2,
      "name": "Piper TTS",
      "description": "High-quality neural voices",
      "requiresBundle": true,
      "binaryPath": {
        "win32": "voices/piper/piper.exe",
        "darwin": "voices/piper/piper",
        "linux": "voices/piper/piper"
      },
      "modelsPath": "voices/piper/models"
    },
    "espeak": {
      "enabled": false,
      "priority": 3,
      "name": "eSpeak-NG",
      "description": "Lightweight, fast synthesis",
      "requiresBundle": true,
      "binaryPath": {
        "win32": "voices/espeak-ng/espeak-ng.exe",
        "darwin": "voices/espeak-ng/espeak-ng",
        "linux": "voices/espeak-ng/espeak-ng"
      },
      "dataPath": "voices/espeak-ng/espeak-ng-data"
    }
  },
  "defaultVoices": {
    "en-US": "webspeech:Microsoft David Desktop",
    "en-GB": "webspeech:Microsoft Hazel Desktop"
  }
}
